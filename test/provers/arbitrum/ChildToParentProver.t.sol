    function _getStorageProofBroadcast() internal pure returns (bytes memory) {
        // Nodes sourced from test/proofs/arbitrum/proof_broadcast.json (storageProof)
        bytes[] memory storageProofList = new bytes[](1);
        storageProofList[0] =
